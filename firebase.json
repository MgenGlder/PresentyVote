{
  "hosting": {
    "headers": [{
      "source": "**/*",
      "headers": [
        {"key": "X-Frame-Options", "value": "SAMEORIGIN"}
      ]
    }],
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
